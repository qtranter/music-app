<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="oval">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/transparent" />
            <stroke
                android:width="1dp"
                android:color="#1Affffff" />
            <padding
                android:bottom="1dp"
                android:left="1dp"
                android:right="1dp"
                android:top="1dp" />
        </shape>
    </item>
</layer-list>